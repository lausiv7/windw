{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es2017",
    "outDir": "out",
    "sourceMap": true,
    "strict": true,
    "rootDir": "src",
    "esModuleInterop": true,
    "downlevelIteration": true,
    "skipLibCheck": true
  },
  "exclude": [
    "node_modules",
    ".vscode-test",
    "src/test.backup/**/*",
    "src/core/*.backup"
  ]
}
