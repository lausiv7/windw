{
  "name": "windwalker",
  "displayName": "WindWalker IDE",
  "description": "AI-powered web development environment",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "windwalker-sidebar",
          "title": "WindWalker",
          "icon": "media/icon.svg"
        }
      ]
    },
    "views": {
      "windwalker-sidebar": [
        {
          "id": "windwalker.aiChat",
          "name": "AI Assistant",
          "type": "webview"
        },
        {
          "id": "windwalker.preview",
          "name": "Live Preview",
          "type": "webview"
        }
      ]
    },
    "commands": [
      {
        "command": "windwalker.helloWorld",
        "title": "WindWalker: Hello World"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "typescript": "^5.3.3"
  }
}
